                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/LogOut">LogOut</a>
                    </li>
                    <li class="nav-item" style="color:white;" id="login_li">
                        <script>
                            let userData = {};
                            var login_li = document.getElementById("login_li");
                            try {
                                userData = { key: sessionStorage.getItem('account') };
                            } catch (e) {
                                location.href = "/";
                            }
                            login_li.innerHTML = "<a class=nav-link><strong>" + userData.key + "</strong> 님 안녕하세요!</a>";

                        </script>
                    </li>

            </div>
        </div>
    </nav>

    <!-- Page Content -->
    <div class="container">

        <!-- Jumbotron Header -->
        <header class="jumbotron my-4">
            <script>
                sessionStorage.removeItem('account');
            </script>

